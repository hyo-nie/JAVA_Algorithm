import java.io.*;

public class Main30 {

	public static void main(String[] args) {
		
		try {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		
		int cnt = 0;
		
		while(true) {
			String jay = br.readLine();
			
			if(jay == null)
			break;
			cnt++;
		}
		br.close();
		System.out.println(cnt);
		
    }catch (Exception e) {
    		}
	}
}
