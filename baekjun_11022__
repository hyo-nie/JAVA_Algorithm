public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int A = sc.nextInt();
		int B = sc.nextInt();
		
		for(A =1; A < 10; A++) {
			System.out.println(A);
		}
		for(B=1; B < 10; B++) {
			System.out.println(B);
		}
		System.out.println("Case #"+ A + ":" + A + "+" +B + "=" + (A+B));
	}

}
