
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		for (int x = 1; x <= n; x++) {
			int a = sc.nextInt();
			for(int b = 1; b <= a; b++) {
			 System.out.printf("=");
			}
			System.out.println();
		}
		sc.close();
	}
}
