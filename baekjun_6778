import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int x = sc.nextInt();
		int y = sc.nextInt();
		sc.close();
			
			if(x>=3 && y<=4) {
				System.out.println("TroyMartian");
			}if(x<=6 && y>=2) {
				System.out.println("VladSaturnian");
			}if(x<=2 && y<=3) {
				System.out.println("GraemeMercurian");
			}
			}
		}
